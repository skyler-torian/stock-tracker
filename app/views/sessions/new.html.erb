


<div class="wrapper">
    <h3>Dont have an account? <%=link_to "Signup", new_user_path%></h3>
    <% if flash[:notice] %>
        <div class="error"><%= flash.notice %></div>
    <% end %>
  
    <%= form_tag "/login" do %>
        <%= label :username, "username"%>
        <%= text_field_tag :username%>

        <%= label :password, "password"%>
        <%= password_field_tag :password%>

        <%= submit_tag "Login"%>
        
    <% end %>
</div>